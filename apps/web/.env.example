# LightLink DEX Configuration
# Copy this to .env.local and fill in values

# Trading API (our custom API on Railway)
TRADING_API_URL_OVERRIDE=https://uniswap-api-production-becb.up.railway.app
REACT_APP_TRADING_API_URL_OVERRIDE=https://uniswap-api-production-becb.up.railway.app
TRADING_API_KEY=lightlink-dex
REACT_APP_TRADING_API_KEY=lightlink-dex

# Uniswap API key (placeholder - not used for LightLink)
UNISWAP_API_KEY=none

# GraphQL endpoint (placeholder - LightLink uses backendSupported=false)
REACT_APP_AWS_API_ENDPOINT=https://uniswap-api-production-becb.up.railway.app/graphql
