{
  "buildCommand": "npx nx build:production web",
  "outputDirectory": "apps/web/build/client",
  "installCommand": "bun install",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
