{
  "framework": null,
  "buildCommand": "npx nx run @universe/web:build:production",
  "outputDirectory": "apps/web/build/client",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
